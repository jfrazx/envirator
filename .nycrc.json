{
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "per-file": true,
  "lines": 99,
  "produce-source-map": true,
  "statements": 99,
  "functions": 99,
  "branches": 95,
  "reporter": ["lcov", "text-summary", "html"],
  "include": ["src/**/*.ts"],
  "exclude": ["test/**/*.spec.ts"],
  "all": true,
  "cache": true,
  "report-dir": "./coverage",
  "tmp-directory": ".nyc_output"
}
